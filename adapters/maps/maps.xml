<wl:adapter name="maps"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:wl="http://www.ibm.com/mfp/integration"
	xmlns:http="http://www.ibm.com/mfp/integration/http">

	<displayName>maps</displayName>
	<description>maps</description>
	<connectivity>
		<connectionPolicy xsi:type="http:HTTPConnectionPolicyType">
			<protocol>http</protocol>
			<domain>maps.googleapis.com</domain>
			<port>-1</port>
			<connectionTimeoutInMilliseconds>30000</connectionTimeoutInMilliseconds>
			<socketTimeoutInMilliseconds>30000</socketTimeoutInMilliseconds>
			<proxy>
				<protocol>http</protocol>
				<domain>proxypar.orleanssrv.domsma</domain>
				<port>8090</port>
			</proxy>
			<maxConcurrentConnectionsPerNode>50</maxConcurrentConnectionsPerNode>			
		</connectionPolicy>
	</connectivity>


	<procedure name="getMaps"> </procedure>
	<procedure name="addMap"> </procedure>
	<procedure name="updateMap"> </procedure>
	<procedure name="deleteMap"> </procedure>
	
	

	
	
</wl:adapter>
