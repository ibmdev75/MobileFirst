<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wl:adapter xmlns:wl="http://www.ibm.com/mfp/integration" xmlns:http="http://www.ibm.com/mfp/integration/http" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Soap_Personne">
    <displayName>Soap_Personne</displayName>
    <description>Service SO de lecture des informations personne</description>
    <connectivity>
        <connectionPolicy xsi:type="http:HTTPConnectionPolicyType">
            <protocol>http</protocol>
            <domain>bazarect.orleanssrv.domsma</domain>
            <port>-1</port>
			<connectionTimeoutInMilliseconds>30000</connectionTimeoutInMilliseconds>
			<socketTimeoutInMilliseconds>30000</socketTimeoutInMilliseconds>
			<maxConcurrentConnectionsPerNode>2</maxConcurrentConnectionsPerNode>
        </connectionPolicy>
    </connectivity>

    <procedure name="DonneesClientServiceImplService_rechercherSouscripteurClient"/>
    <procedure name="DonneesClientServiceImplService_lireInformationsPersonne"/>
</wl:adapter>
